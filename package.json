{
  "name": "node",
  "version": "1.0.0",
  "description": "A node template project",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "precommit": "npm run lint:fix && npm run pretty",
    "prepush": "npm run lint",
    "test": "jest",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "pretty": "prettier --write src/**/*.ts test/**/*.ts",
    "build": "tsc",
    "start": "node index.js",
    "dev": "ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AstralJaeger/node.git"
  },
  "keywords": [
    "node",
    "template"
  ],
  "author": "Felix Hillebrand <astraljaeger@pm.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AstralJaeger/node/issues"
  },
  "homepage": "https://github.com/AstralJaeger/node#readme",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-spellcheck": "^0.0.20",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "jsdoc": "^4.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "env-var": "^7.3.0",
    "log4js": "^6.8.0"
  }
}
