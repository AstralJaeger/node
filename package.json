{
  "name": "node",
  "version": "1.0.0",
  "description": "A node template project",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run sbom",
    "prepare": "husky install",
    "precommit": "npm run lint:fix && npm run pretty",
    "prepush": "npm run lint",
    "test": "jest",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "pretty": "prettier --write src/**/*.ts test/**/*.ts",
    "sbom": "cyclonedx-npm --output-reproducible --output-file bom.json",
    "build": "tsc",
    "start": "node index.js",
    "dev": "ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AstralJaeger/node.git"
  },
  "keywords": [
    "node",
    "template"
  ],
  "author": "Felix Hillebrand <astraljaeger@pm.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AstralJaeger/node/issues"
  },
  "homepage": "https://github.com/AstralJaeger/node#readme",
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "1.13.2",
    "@types/jest": "29.5.4",
    "@types/node": "20.5.7",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "eslint": "8.48.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-spellcheck": "0.0.20",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "jsdoc": "4.0.2",
    "prettier": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "env-var": "7.4.1",
    "log4js": "6.9.1"
  }
}
